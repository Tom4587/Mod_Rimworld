<?xml version="1.0" encoding="utf-8" ?>
<Defs>  


	<ThingDef ParentName="RoughPlantBase">
		<defName>VCE_UncookedSoupSimple</defName>
		<label>uncooked simple soup</label>
		<description>An uncooked simple soup. This needs to be cooked in an electric pot to be edible.</description>
		<graphicData>
			<texPath>Things/Items/Uncookedsoup/VCE_UncookedSoupSimple</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>120</MarketValue>
			<Flammability>1.3</Flammability>
			<WorkToMake>300</WorkToMake>
		</statBases>
		<tradeability>None</tradeability>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>20</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
			<li Class="CompProperties_Ingredients" />		
			<li Class="VanillaCookingExpanded.CompProperties_StackByIngredients" />			
		</comps>
		<thingCategories>
			<li>VCE_UncookedSoups</li>
		</thingCategories>
		<ingestible>
			<preferability>NeverForNutrition</preferability>			
		</ingestible>

	</ThingDef>

	<ThingDef ParentName="MealCooked">
		<defName>VCE_CookedSoupSimple</defName>
		<label>simple soup</label>
		<description>A simple soup slow cooked from a few ingredients.</description>
		<stackLimit>20</stackLimit>
		<graphicData>
			<texPath>Things/Meals/VCE_SoupSimple</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>12</MarketValue>
			<Nutrition>0.6</Nutrition>
		</statBases>
		<ingestible>
			<preferability>MealSimple</preferability>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Ingest_Drink</ingestSound>
		</ingestible>

	</ThingDef>

